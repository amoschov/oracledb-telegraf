stream
   |from()
      .measurement('oracle_fra_pctused')
      .groupBy('host')
   |window()
      .period(60s)
      .every(60s)
   |mean('fra_pctused')
   |eval(lambda: "mean")
      .as('fra_pct')
   |alert()
      .id('oracle_fra')
      .message('{{ .Level}}: {{ .Name }}/{{ index .Tags "host" }} has high fra usage: {{ index .Fields "fra_pct" }}')
      .warn(lambda: "fra_pct" > 0) 
      .crit(lambda: "fra_pct" > 95) 
      .log('/var/log/kapacitor/fra.log')
